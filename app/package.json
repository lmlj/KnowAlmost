{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch": "rm -rf public && webpack --progress --color -w -d",
    "watchDev": "rm -rf public && cross-env serverEnv=dev webpack --progress --color -w -d",
    "analysis": "webpack --progress -p --json --profile > analysis.json",
    "build": "rm -rf public  && cross-env NODE_ENV=production webpack --progress --color -p",
    "buildDev": "rm -rf public  && cross-env NODE_ENV=production serverEnv=dev webpack --progress --color -p"
  },
  "dependencies": {
    "antd": "^2.12.3",
    "classnames": "^2.2.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.3",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "happypack": "^4.0.0-beta.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
